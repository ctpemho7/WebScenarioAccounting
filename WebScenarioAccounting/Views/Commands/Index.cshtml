@model IEnumerable<WebScenarioAccounting.Command>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.isElse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommandID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actuator.Manufacturer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Scenario.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.isElse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommandID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actuator.Manufacturer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Scenario.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
